<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/btnUtilizacion"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondohome"
    android:maxWidth="1000dp"
    android:maxHeight="1000dp"
    tools:context="com.trantec.yo.ui.main.HomeActivity">

    <Button
        android:id="@+id/BtnEnrollment"
        android:layout_width="114dp"
        android:layout_height="105dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/icon1"
        app:layout_constraintBottom_toTopOf="@+id/BtnMyStores"
        app:layout_constraintEnd_toStartOf="@+id/BtnUtilizacion"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/BtnUtilizacion"
        android:layout_width="114dp"
        android:layout_height="105dp"
        android:layout_marginEnd="56dp"
        android:background="@drawable/icon2"
        app:layout_constraintBottom_toTopOf="@+id/BtnReport"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.913" />

    <Button
        android:id="@+id/BtnMyStores"
        android:layout_width="114dp"
        android:layout_height="105dp"
        android:layout_marginStart="56dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="182dp"
        android:background="@drawable/icon3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/BtnReport"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/BtnReport"
        android:layout_width="114dp"
        android:layout_height="105dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="182dp"
        android:background="@drawable/icon4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/btnRopa"
        android:layout_width="123dp"
        android:layout_height="51dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="8dp"
        android:contentDescription="@string/nav_header_desc"
        android:paddingTop="@dimen/nav_header_vertical_spacing"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCelular2"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ropa" />

    <ImageView
        android:id="@+id/btnCelular2"
        android:layout_width="118dp"
        android:layout_height="59dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="52dp"
        android:contentDescription="@string/nav_header_desc"
        android:paddingTop="@dimen/nav_header_vertical_spacing"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/BtnReport"
        app:srcCompat="@drawable/celulares" />

    <ImageView
        android:id="@+id/btnDeports2"
        android:layout_width="115dp"
        android:layout_height="110dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:contentDescription="@string/nav_header_desc"
        android:paddingTop="@dimen/nav_header_vertical_spacing"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/BtnMyStores"
        app:srcCompat="@drawable/deportes" />


    <TextView
        android:id="@+id/textView16"
        android:layout_width="132dp"
        android:layout_height="26dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="32dp"
        android:text="@string/welcome"
        android:textColor="#ffffff"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.82"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="179dp"
        android:layout_height="25dp"
        android:text="@string/avalaible_money"
        android:textAlignment="textStart"
        android:textColor="#ffffff"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textAmount"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.878"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textAmount"
        android:layout_width="132dp"
        android:layout_height="26dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="484dp"
        android:text="$0"
        android:textAlignment="textStart"
        android:textColor="#ffffff"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="95dp"
        android:layout_height="88dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="28dp"
        android:layout_marginBottom="8dp"
        android:contentDescription="@string/nav_header_desc"
        android:paddingTop="@dimen/nav_header_vertical_spacing"
        app:layout_constraintBottom_toTopOf="@+id/BtnEnrollment"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/logoyopresto" />

    <TextView
        android:id="@+id/textName"
        android:layout_width="132dp"
        android:layout_height="26dp"
        android:text="Yo Presto"
        android:textAlignment="textStart"
        android:textColor="#ffffff"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="207dp"
        tools:layout_editor_absoluteY="45dp" />


</android.support.constraint.ConstraintLayout>



