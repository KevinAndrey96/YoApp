<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/btnUtilizacion"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondohome"
    tools:context="com.trantec.yo.ui.main.HomeActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="97dp"
        android:layout_height="104dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="36dp"
        android:layout_marginTop="37dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/logoyopresto"
        android:contentDescription="@string/nav_header_desc"
        android:paddingTop="@dimen/nav_header_vertical_spacing" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="132dp"
            android:layout_height="26dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="56dp"
            android:layout_marginEnd="63dp"
            android:text="@string/welcome"
            android:textColor="#ffffff"
            android:textSize="20sp"
            android:textStyle="bold" />

    <TextView
        android:id="@+id/txtName"
        android:layout_width="174dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView16"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="22dp"
        android:text="YoPresto"
        android:textColor="#ffffff"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/txtAccount"
        android:layout_width="132dp"
        android:layout_height="26dp"
        android:layout_below="@+id/textView16"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="64dp"
        android:text="1"
        android:textColor="#ffffff"
        android:textSize="16sp" />

    <Button
        android:id="@+id/BtnEnrollment"
        android:layout_width="114dp"
        android:layout_height="105dp"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="56dp"
        android:layout_marginTop="94dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/icon1" />

    <Button
        android:id="@+id/BtnUtilizacion"
        android:layout_width="114dp"
        android:layout_height="105dp"
        android:layout_below="@+id/txtName"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="125dp"
        android:layout_marginEnd="57dp"
        android:background="@drawable/icon2" />

    <Button
        android:id="@+id/BtnMyStores"
        android:layout_width="114dp"
        android:layout_height="105dp"
        android:layout_below="@+id/BtnEnrollment"
        android:layout_alignParentStart="true"
        android:layout_marginStart="56dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/icon3" />

    <Button
        android:id="@+id/BtnReports"
        android:layout_width="114dp"
        android:layout_height="105dp"
        android:layout_below="@+id/BtnEnrollment"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="58dp"
        android:background="@drawable/icon4" />

    <ImageView
        android:id="@+id/btnDeports2"
        android:layout_width="115dp"
        android:layout_height="110dp"
        android:layout_below="@+id/BtnMyStores"
        android:layout_marginStart="56dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/deportes"
        android:contentDescription="@string/nav_header_desc"
        android:paddingTop="@dimen/nav_header_vertical_spacing" />

    <ImageView
        android:id="@+id/btnRopa"
        android:layout_width="123dp"
        android:layout_height="51dp"
        android:layout_below="@id/btnCelular"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="58dp"
        android:background="@drawable/ropa"
        android:contentDescription="@string/nav_header_desc"
        android:paddingTop="@dimen/nav_header_vertical_spacing" />

    <ImageView
        android:id="@+id/btnCelular"
        android:layout_width="123dp"
        android:layout_height="51dp"
        android:layout_below="@+id/BtnReports"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="58dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/celulares"
        android:contentDescription="@string/nav_header_desc"
        android:paddingTop="@dimen/nav_header_vertical_spacing" />
</RelativeLayout>
